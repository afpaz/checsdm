<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated Mappings -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="ca.ets.sofeess.checsdm.mappings.presentation.MappingsModelWizardID"
            name="%_UI_MappingsModelWizard_label"
            class="ca.ets.sofeess.checsdm.mappings.presentation.MappingsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MappingsModelFile.gif">
         <description>%_UI_MappingsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated Mappings -->
      <editor
            id="ca.ets.sofeess.checsdm.mappings.presentation.MappingsEditorID"
            name="%_UI_MappingsEditor_label"
            icon="icons/full/obj16/MappingsModelFile.gif"
            extensions="mappings"
            class="ca.ets.sofeess.checsdm.mappings.presentation.MappingsEditor"
            contributorClass="ca.ets.sofeess.checsdm.mappings.presentation.MappingsActionBarContributor">
      </editor>
   </extension>

</plugin>
